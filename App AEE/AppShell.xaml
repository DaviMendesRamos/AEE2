<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="App_AEE.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:App_AEE"
    xmlns:pages="clr-namespace:App_AEE.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="App_AEE">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent Title="Home"
             ContentTemplate="{DataTemplate pages:HomePrincipalPage }"
             Route="HomePrincipalPage"/>
        
        <ShellContent Title="Busca"
      ContentTemplate="{DataTemplate pages:BuscaUsuarioPage }"
      Route="BuscaUSuarioPage"/>
        
        <ShellContent Title="Lista"
      ContentTemplate="{DataTemplate pages:ListaFeedPage }"
      Route="ListaFeedPage"/>


    </FlyoutItem>
    

</Shell>
